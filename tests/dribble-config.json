{
    "activeConfig":"debug",
    "rootPath":"./logs",
    "fileDateFormat":"YYYY_MM_DD",
    "production":{

    },
    "debug":{
        "error":{
            "transport":["console", "file"],
            "format": "plain",
            "color":"red",
            "path":"/{date}_error_{target}.log",
            "include":["stack"]
        },
        "warning":{
            "transport":["console"],
            "color":"yellow"
        },
        "info":{
            "transport":["console", "file"],
            "color":"green",
            "format":"json",
            "disabled":false,
            "path":"/{date}_info_{target}.log"
        },
        "milestone":{
            "transport":["console"],
            "color":"white"
        },
        "console":{
            "disabled":false
        }

    }
}